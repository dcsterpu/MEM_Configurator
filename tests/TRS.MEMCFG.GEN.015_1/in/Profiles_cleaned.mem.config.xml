<CONFIG_PSA_NVM xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="..\..\..\..\TOOLS\xsd\ConfigPSA_NvM.xsd">
  <PROFILS>
    <PROFILE>
      <SHORT-NAME>CfgVh_1</SHORT-NAME>
      <MANAGEMENT>IMPLICIT</MANAGEMENT>
      <DURABILITY>LONG_TERM</DURABILITY>
      <BLOCK-SIZE-MAX>1000</BLOCK-SIZE-MAX>
      <SAFETY>true</SAFETY>
      <DEVICE>EA</DEVICE>
      <WRITING-NUMBER>1500</WRITING-NUMBER>
      <PARAMS>
        <PARAM DEST="NvMBlockJobPriority">1</PARAM>
        <PARAM DEST="NvMBlockCrcType">NVM_CRC16</PARAM>
        <PARAM DEST="NvMBlockManagementType">NVM_BLOCK_REDUNDANT</PARAM>
        <PARAM DEST="NvMRedundantRecovery">NVM_RECOVERY_ON_REQUEST</PARAM>
        <PARAM DEST="NvMBlockUseAutoValidation">False</PARAM>
        <PARAM DEST="NvMBlockUseCRCCompMechanism">False</PARAM>
        <PARAM DEST="NvMBlockUseSetRamBlockStatus">True</PARAM>
        <PARAM DEST="NvMBlockUseSyncMechanism">True</PARAM>
        <PARAM DEST="NvMBlockWriteProt">False</PARAM>
        <PARAM DEST="NvMBswMBlockStatusInformation">False</PARAM>
        <PARAM DEST="NvMCalcRamBlockCrc">True</PARAM>
        <PARAM DEST="NvMInitBlockCallback">False</PARAM>
        <PARAM DEST="NvMMaxNumOfReadRetries">3</PARAM>
        <PARAM DEST="NvMMaxNumOfWriteRetries">3</PARAM>
        <PARAM DEST="NvMNvBlockNum">2</PARAM>
        <PARAM DEST="NvMResistantToChangedSw">False</PARAM>
        <PARAM DEST="NvMRomBlockNum">2</PARAM>
        <PARAM DEST="NvMSelectBlockForReadAll">True</PARAM>
        <PARAM DEST="NvMSelectBlockForWriteAll">False</PARAM>
        <PARAM DEST="NvMStaticBlockIDCheck">True</PARAM>
        <PARAM DEST="NvMWriteBlockOnce">False</PARAM>
        <PARAM DEST="NvMWriteVerification">True</PARAM>
        <PARAM DEST="NvMWriteVerificationDataSize">4</PARAM>
        <PARAM DEST="NvMWriteRamBlockToNvCallback">True</PARAM>
        <PARAM DEST="NvMReadRamBlockFromNvCallback">True</PARAM>
        <PARAM DEST="NvMNvramDeviceId">0</PARAM> <!-- index id of pluing EA = 0 -->
      </PARAMS>
    </PROFILE>
